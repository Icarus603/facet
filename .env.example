# FACET Platform Environment Variables

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_INDEX_NAME=facet-memory

# Redis for Session Management
REDIS_URL=redis://localhost:6379

# Next.js Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# AI Model Configuration (GPT-5 for all agents)
DEFAULT_MODEL=gpt-5-2025-08-07
ORCHESTRATOR_MODEL=gpt-5-2025-08-07
EMOTION_ANALYZER_MODEL=gpt-5-2025-08-07
MEMORY_MANAGER_MODEL=gpt-5-2025-08-07
CRISIS_MONITOR_MODEL=gpt-5-2025-08-07
THERAPY_ADVISOR_MODEL=gpt-5-2025-08-07
PROGRESS_TRACKER_MODEL=gpt-5-2025-08-07

# LangChain Configuration
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=facet-agents
LANGCHAIN_TRACING_V2=true

# Agent Performance Configuration
ORCHESTRATOR_TIMEOUT_MS=5000
AGENT_PARALLEL_TIMEOUT_MS=3000
CRISIS_RESPONSE_TIMEOUT_MS=2000
MAX_PARALLEL_AGENTS=4

# Agent Coordination Settings
ENABLE_PARALLEL_EXECUTION=true
ENABLE_AGENT_TRANSPARENCY=true
DEFAULT_TRANSPARENCY_LEVEL=standard

# Development/Production Mode
NODE_ENV=development